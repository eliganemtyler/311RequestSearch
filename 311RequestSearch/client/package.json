{
  "name": "311RequestSearch",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npm run ngcc",
    "ng": "ng",
    "ngcc": "ngcc",
    "start": "env-cmd -f ../.env --use-shell \"npm run serve\"",
    "build": "npm run build:client:Release",
    "test": "ng test --code-coverage --browsers=ChromeHeadless",
    "test:debug": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ng extract-i18n --output-path src/locale",
    "serve": "cross-env-shell ng serve --configuration=development --host=$TCP_HOSTNAME --port=$CLIENT_DEV_SERVER_PORT",
    "build:client:Debug": "ng build --configuration development --localize",
    "build:client:Release": "ng build --configuration production --localize",
    "prebuild:client:Debug": "npm run clean:build",
    "prebuild:client:Release": "npm run clean:build",
    "postbuild:client:Release": "node ./scripts/post-build-hash.js",
    "postbuild:client:Debug": "node ./scripts/post-build-hash.js",
    "compodoc": "compodoc -p tsconfig.compodoc.json --theme vagrant",
    "clean:build": "rimraf build",
    "clean:documentation": "rimraf documentation",
    "compodoc:watch": "npm run compodoc -- -s -w",
    "echo:creation": "echo \"Generated on 09/08/2023 with Armored Armadillo 1.0.0-dev\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.5",
    "@angular/common": "^14.0.5",
    "@angular/compiler": "^14.0.5",
    "@angular/core": "^14.0.5",
    "@angular/forms": "^14.0.5",
    "@angular/platform-browser": "^14.0.5",
    "@angular/platform-browser-dynamic": "^14.0.5",
    "@angular/router": "^14.0.5",
    "@babel/plugin-transform-template-literals": "^7.13.0",
    "@datadog/browser-rum": "^1.26.3",
    "@tylertech-eslint/eslint-plugin": "1.0.2",
    "@tylertech-eslint/eslint-plugin-angular": "1.0.2",
    "@tylertech-eslint/eslint-plugin-angular-template": "1.0.2",
    "@tylertech/forge": "^2.0.0",
    "@tylertech/forge-angular": "^2.0.0",
    "@tylertech/forge-internal": "^2.0.0",
    "@tylertech/tyler-cookie-consent": "^1.0.1",
    "@tylertech/tyler-icons": "^1.12.0",
    "karma-spec-reporter": "0.0.32",
    "ngx-cookie-service": "^14.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.5",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^14.0.5",
    "@angular/compiler-cli": "^14.0.5",
    "@angular/localize": "^14.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsdoc": "^34.7.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",

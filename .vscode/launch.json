{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build dotnet core",
      "program": "${workspaceFolder}/311RequestSearch/bin/Debug/net6.0/311RequestSearch.dll",
      "args": [],
      "cwd": "${workspaceFolder}/311RequestSearch",
      "stopAtEntry": false,
      "envFile": "${workspaceFolder}/311RequestSearch/.env"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    },
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://dev.localdev.tcpci.com:5000/app/new-application",
      "webRoot": "${workspaceFolder}/311RequestSearch/client"
    },
    {
      "name": "Launch Firefox",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://dev.localdev.tcpci.com:5000/app/new-application",
      "webRoot": "${workspaceFolder}/311RequestSearch/client"
    },
    {
      "name": "Launch Edge",
      "type": "edge",
      "request": "launch",
      "url": "http://dev.localdev.tcpci.com:5000/app/new-application",
      "webRoot": "${workspaceFolder}/311RequestSearch/client"
    },
    {
      "name": "Launch Chrome & Client Server",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "ng serve",
      "postDebugTask": "kill all tasks",
      "url": "http://dev.localdev.tcpci.com:5000/app/new-application",
      "webRoot": "${workspaceFolder}/311RequestSearch/client"
    },
    {
      "name": "Launch Firefox & Client Server",
      "type": "firefox",
      "request": "launch",
      "preLaunchTask": "ng serve",
      "postDebugTask": "kill all tasks",
      "reAttach": true,
      "url": "http://dev.localdev.tcpci.com:5000/app/new-application",
      "webRoot": "${workspaceFolder}/311RequestSearch/client"
    },
    {
      "name": "Launch Edge & Client Server",
      "type": "edge",
      "request": "launch",
      "preLaunchTask": "ng serve",
      "postDebugTask": "kill all tasks",
      "url": "http://dev.localdev.tcpci.com:5000/app/new-application",
      "webRoot": "${workspaceFolder}/311RequestSearch/client"
    }
  ]
}
